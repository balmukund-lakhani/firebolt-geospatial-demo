# Firebolt Geospatial Demo Configuration
# Copy this file to .env and fill in your Firebolt credentials

# Firebolt Connection Details
FIREBOLT_CLIENT_ID=your_client_id
FIREBOLT_CLIENT_SECRET=your_client_secret
FIREBOLT_ACCOUNT_NAME=your_account_name
FIREBOLT_DATABASE=your_database_name
FIREBOLT_ENGINE=your_engine_name

# Optional: API Endpoint (defaults to api.app.firebolt.io)
# FIREBOLT_API_ENDPOINT=api.app.firebolt.io
STREAMLIT_SERVER_ADDRESS=localhost